{
  "name": "nouns-agora",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@nouns/assets": "^0.4.0",
    "@nouns/sdk": "^0.2.0",
    "@restart/ui": "^1.3.1",
    "@tanstack/react-query": "^4.2.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "ethers": "^5.7.0",
    "mdast-util-from-markdown": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "^14.1.0",
    "remark-breaks": "^3.0.2",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.graphql",
    "language": "typescript"
  },
  "scripts": {
    "start": "yarn run codegen && react-scripts start",
    "build": "yarn run codegen && react-scripts build",
    "relay": "yarn run relay-compiler",
    "generate-schema": "ts-node src/graphql/generateScheama.ts > schema.graphql",
    "backend": "ts-node src/grpahql/server.ts",
    "typechain-generate": "typechain --target ethers-v5 --out-dir src/contracts/generated src/abis/*.json",
    "codegen": "yarn run generate-schema && yarn run relay && yarn run typechain-generate",
    "fetch-schema": "get-graphql-schema https://api.thegraph.com/subgraphs/name/nounsdao/nouns-subgraph > ./src/graphql/schemas/nouns-subgraph.graphql",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.3",
    "@graphql-tools/load": "^7.7.5",
    "@graphql-tools/stitch": "^8.7.7",
    "@graphql-yoga/node": "^2.13.12",
    "@nouns/contracts": "^0.1.3",
    "@tailwindcss/typography": "^0.5.7",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/react-relay": "^14.1.1",
    "@types/relay-runtime": "^14.1.2",
    "autoprefixer": "^10.4.8",
    "babel-plugin-relay": "^14.1.0",
    "get-graphql-schema": "^2.1.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "relay-compiler": "^14.1.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0"
  }
}
